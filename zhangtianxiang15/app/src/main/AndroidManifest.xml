        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.Login"></activity>
        <activity android:name=".Login.Register"></activity>
        <activity android:name=".Page.Selectaction"></activity>
        <activity android:name=".Page.AddStudent"></activity>
